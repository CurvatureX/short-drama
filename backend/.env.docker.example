# ==================== AWS Configuration ====================
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY=your-aws-access-key
AWS_ACCESS_SECRET=your-aws-secret-key
AWS_SESSION_TOKEN=

# ==================== S3 & CloudFront ====================
S3_BUCKET_NAME=your-s3-bucket
CLOUDFRONT_DOMAIN=https://your-cloudfront-domain.cloudfront.net

# ==================== Supabase Configuration ====================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_PUB_KEY=your-publishable-key
SUPABASE_SECRET_KEY=your-service-role-key

# ==================== Canvas Service Configuration ====================
CORS_ORIGINS=http://localhost:3000
PRESIGN_EXPIRY_SECONDS=3600

# ==================== Orchestrator Configuration ====================
# GPU Task Queue
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT/gpu_tasks_queue

# CPU Task Queue
CPU_SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT/cpu_tasks_queue

# DynamoDB Table
DYNAMODB_TABLE=task_store

# GPU EC2 Instance
GPU_INSTANCE_ID=i-xxxxxxxxxxxxx

# ==================== External API Keys ====================
# Dashscope (for Qwen models)
DASHSCOPE_API_KEY=your-dashscope-api-key

# BRIA API
BRIA_API_KEY=your-bria-api-key

# Volcano Engine ARK API (for SeeDream)
ARK_API_KEY=your-ark-api-key
